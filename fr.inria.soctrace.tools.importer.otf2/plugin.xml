<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="fr.inria.soctrace.framesoc.core.tool">
      <tool
            class="fr.inria.soctrace.tools.importer.otf2.Otf2Importer"
            doc="Select the OTF2 anchor file. Options: -novar (ignore variables), -ph (parse hierarchy)."
            id="fr.inria.soctrace.tools.importer.otf2"
            name="OTF2 Importer"
            type="IMPORT">
      </tool>
   </extension>
   <extension
         point="fr.inria.soctrace.framesoc.ui.input.toolInput">
      <toolInput
            compositeFactory="fr.inria.soctrace.tools.importer.otf2.input.Otf2InputCompositeFactory"
            toolId="fr.inria.soctrace.tools.importer.otf2">
      </toolInput>
   </extension>

</plugin>
